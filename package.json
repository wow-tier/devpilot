{
  "name": "devpilot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "setup": "bash scripts/setup.sh",
    "db:setup": "bash database/setup.sh",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:seed": "psql -U postgres -d devpilot < database/seed.sql",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules",
    "verify": "bash scripts/verify-install.sh"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@next/font": "^14.2.15",
    "@prisma/client": "^6.17.1",
    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.1",
    "diff": "^5.2.0",
    "glob": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.546.0",
    "next": "15.5.5",
    "next-auth": "^4.24.11",
    "openai": "^4.77.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "simple-git": "^3.25.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^5.0.2",
    "@types/diff": "^5.2.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/wouter": "^2.0.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "prisma": "^6.17.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
